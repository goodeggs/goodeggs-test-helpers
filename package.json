{
  "name": "goodeggs-test-helpers",
  "version": "6.2.1",
  "description": "Basic setup used for all goodeggs tests.",
  "main": "lib/index.js",
  "browser": {
    "./lib/index.js": "./lib/browser.js",
    "sinon": "./lib/sinon_webpack_shim.js"
  },
  "author": "Good Eggs Inc.",
  "contributors": [
    "dannynelson <danny@goodeggs.com>",
    "max edmands <max@goodeggs.com>"
  ],
  "license": "UNLICENSED",
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/goodeggs/goodeggs-test-helpers.git"
  },
  "dependencies": {
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chai-datetime": "1.5.0",
    "chaid": "1.0.2",
    "date-test-helpers": "1.1.2",
    "dirty-chai": "2.0.1",
    "istanbul": "1.0.0-alpha.2",
    "mocha-multi": "1.0.1",
    "mocha-osx-reporter": "0.1.2",
    "sinon": "4.4.0",
    "sinon-chai": "2.14.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.6",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.26.0",
    "bunyan": "1.8.12",
    "eslint": "4.19.1",
    "eslint-plugin-goodeggs": "6.1.1",
    "flow-bin": "^0.78.0",
    "mocha": "5.2.0"
  },
  "peerDependencies": {
    "babel-register": "*",
    "mocha": ">=5"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "transform-flow-strip-types"
    ]
  },
  "scripts": {
    "build": "rm -rf lib/ && babel src -d lib && cp src/index.js.flow lib/",
    "prepublishOnly": "yarn build",
    "lint": "eslint 'src/**/*.js'  --ignore-path .gitignore",
    "test:mocha": "./runtest.sh",
    "test:watch": "./runtest.sh --watch",
    "test:coverage": "./runtest.sh --coverage",
    "test": "yarn run lint && flow && npm run test:mocha"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "always-auth": true
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "goodeggs"
    ],
    "extends": [
      "plugin:goodeggs/goodeggs"
    ],
    "env": {
      "node": true
    }
  },
  "bin": {
    "runtest": "./runtest.sh"
  }
}
