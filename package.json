{
  "name": "goodeggs-test-helpers",
  "version": "7.0.0",
  "description": "Basic setup used for all goodeggs tests.",
  "author": "Good Eggs Inc.",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/goodeggs/goodeggs-test-helpers.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "always-auth": true
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "yarn run build:clean && yarn run build:transpile",
    "build:clean": "rm -rf lib",
    "build:transpile": "babel src --out-dir lib --copy-files",
    "lint": "yarn run lint:es",
    "lint:es": "getk run lint-es",
    "lint:fix": "yarn run lint:fix:es",
    "lint:fix:es": "getk run fix-es",
    "prepublishOnly": "yarn run build",
    "postversion": "git push --follow-tags",
    "tdd": "yarn run test:mocha --watch",
    "test": "yarn run todos && yarn run lint && yarn run typecheck && yarn run test:mocha",
    "test:mocha": "yarn run test:mocha:glob 'test/**/*test.js'",
    "test:mocha:glob": "NODE_ENV=test mocha --opts mocha.opts",
    "todos": "yarn run todos:glob '**/*.{js,jsx,ts,tsx}'",
    "todos:glob": "leasot --exit-nicely --tags '\\$FlowFixMe' --ignore='node_modules/**','lib/**','flow-typed/**'",
    "typecheck": "flow"
  },
  "dependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-datetime": "1.7.0",
    "chaid": "1.0.2",
    "date-test-helpers": "1.1.2",
    "dirty-chai": "2.0.1",
    "sinon": "9.0.3",
    "sinon-chai": "3.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@goodeggs/toolkit": "^3.0.1",
    "bunyan": "1.8.12",
    "flow-bin": "^0.143.1",
    "leasot": "^9.1.0",
    "mocha": "^6.2.3"
  }
}
